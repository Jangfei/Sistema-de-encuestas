<h3><%= link_to "Inicio", {:controller => "/start"},{} -%>&rarr; 
<%= link_to "Administrador", {:controller => "/start", :action => "administrador"},{} -%>&rarr;
<%= link_to "Encuesta - #{parent_object.nombre_encuesta}", administrador_surveys_url -%>&rarr;
<%= link_to "Resultados" -%></h3>

<% content_for :sidebar do %>
<div class="info">
    <h2>Opciones</h2>
</div>
<%end %>
<table class="azul">
    <thead>
        <tr>
            <th></th>
            <th>ID resultado</th>
            <th>Fecha de respuesta</th>
        </tr>
    </thead>
    <tbody id="<%= list_id-%>">
        <%= render :partial => "row", :collection=> @collection %>
    </tbody>
     <tfoot id="<%= footer_id%>">
        <tr>
            <th colspan="3">
                <%= render :partial => 'shared/pagination_counter', :locals => {:collection=> @collection} -%>
            </th>
        </tr>
    </tfoot>
</table>
